{
    "name": "kamilkrzywda/date-interval",
    "description": "Provides additional functionality to the DateInterval class.",
    "keywords": ["date", "time", "interval"],
    "homepage": "hhttps://github.com/kamilkrzywda/php-date-interval",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io/"
        }
    ],
    "support": {
        "issues": "https://github.com/kamilkrzywda/php-date-interval/issues"
    },
    "require": {
        "php": ">=5.3",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\DateInterval": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
